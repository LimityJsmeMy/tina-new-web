{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/ljm-header",
	"version": "0.1.0",
	"title": "LJM bold header",
	"category": "widgets",
	"icon": "align-full-width",
	"description": "Limity Jsme My custom bold header widget, made with love by Jirka.",
	"supports": {
		"html": false
	},
	"textdomain": "ljm-header",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./variables.js",
	"attributes": {
		"align": {
			"type": "string",
			"default": "center"
		},
		"title": {
			"type": "string",
			"default": "POWER TO THE PEOPLE!"
		},
		"text_color": {
			"type": "string",
			"default": "rgb(255 56 73)"
		},
		"glitch_color_1": {
			"type": "string",
			"default": "rgb(67 255 111 / 70%)"
		},
		"glitch_color_2": {
			"type": "string",
			"default": "rgb(67 152 255 / 64%)"
		},
		"flickering_color": {
			"type": "string",
			"default": "rgb(0 0 0 / 12%)"
		},
		"image1": {
			"type": "object",
			"default": {
				"url": "https://limityjsmemy.cz/wp-content/uploads/2018/09/10.jpg"
			}
		},
		"image2": {
			"type": "object",
			"default": {
				"url": "https://limityjsmemy.cz/wp-content/uploads/2016/10/13064557_1189359104410080_3148805146143209369_o.jpg"
			}
		}
	}
}