{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/ljm-header",
  "version": "0.1.0",
  "title": "LJM bold header",
  "category": "widgets",
  "icon": "align-full-width",
  "description": "Limity Jsme My custom bold header widget, made with love by Jirka.",
  "supports": {
    "html": false
  },
  "textdomain": "ljm-header",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "POWER TO THE PEOPLE!"
    },
    "bg_image_1": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "https://limityjsmemy.cz/wp-content/uploads/2018/09/10.jpg"
    },
    "bg_image_2": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "https://limityjsmemy.cz/wp-content/uploads/2016/10/13064557_1189359104410080_3148805146143209369_o.jpg"
    },
    "text_color": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "#ff3849"
    },
    "glitch_color_1": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "#4398ffa2"
    },
    "glitch_color_2": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": "#4398ffa2"
    },
    "mediaId": {
      "type": "number",
      "default": 0
    },
    "mediaUrl": {
      "type": "string",
      "default": "http://dev.ccflow.ai/wp-content/themes/custom/assets/images/casette_frame_mockup.png"
    },
    "backgroundColor": {
      "type": "string"
    },
    "textColor": {
      "type": "string"
    }
  }
}